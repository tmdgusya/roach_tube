extends layouts/main
include mixins/videoBlock

block content
    .search__header
        h3 searching for #{searchingFor} 
    .search__videos
        if videos.length === 0
            h5 No Videos Found
        each item in videos
            +videoBlock({
                title: item.title,
                views: item.views,
                videoFile:item.videoFile,
                id: item.id
            })